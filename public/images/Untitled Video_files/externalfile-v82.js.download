                                grecaptcha.execute('6Lf_oWAUAAAAACZ05oUmZpYozdYF7o0Oy4Iy8wnz', {action: 'submit'}).then(function(token) {
                                document.getElementsByClassName("lds-animaker-overlay")[0].style.display = "block";

